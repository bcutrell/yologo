
<% @logos.each do |logo| %>
  <div class = "span3.5 logo_index">

    <ul>
      <li ><%= logo.title %></li>        
      <li><%= image_tag logo.logo.thumb.url, :class => "img-polaroid" %></li>
        <li><%= link_to 'Vote', logo_path(logo), :class =>"btn" %></li>
    </ul>

  </div>
<% end %>
</div>

<div class = >
<%= paginate @logos%>
</div>